                  <a
                    class="nav-link active"
                    aria-current="page"
                    href="/auth/logout"
                  >Logout</a>
                </li>
              {{/if}}
              {{#unless isUserActive}}
                <li class="nav-item">
                  <a
                    class="nav-link active"
                    aria-current="page"
                    href="/auth/login"
                  >Login</a>
                </li>
                <li class="nav-item">
                  <a class="nav-link" href="/auth/signup">Signup</a>
